fn - 0x35 : return
#fn - 0x35 : ~/bin/keyboard_backlight # esc ---> Perhaps change caps_lock in second layer of q1-map from _______ to C(_______)
#f15 : ~/bin/keyboard_backlight # esc [old: f18] ---> karabiner now!! escape now directly to keyboard light toggle... nothing more since escape now is a capsloch/control...
  # fn - f* : special features / standard function keys (stock)

fn - 0x0A : return # ^
fn - 1 : return
  fn - 2 : skhd -t "²"
  fn - 3 : skhd -t "³"
fn - 4 : return
fn - 5 : return
fn - 6 : return
  fn - 7 : skhd -k "alt - 8" # {
  fn - 8 : skhd -k "alt - 5" # [
  fn - 9 : skhd -k "alt - 6" # ]
  fn - 0 : skhd -k "alt - 9" # }
  fn - 0x1B : skhd -k "shift + alt - 7" # \
fn - 0x18 : return # ´
  # fn - backspace/delete : (forward) delete (stock)

fn - tab : return
  fn - q : skhd -k "alt - l" # @, quick note (overwritten here & deactivated in systemSettings/Keyboard/KeyboardShortcuts/.. like many others not mentioned specifically)
fn - w : return
  fn - e : skhd -k "alt - e" # €, show the character viewer, from which you can choose emoji and other symbols (stock)
fn - r : return
fn - t : return
fn - z : return
#fn - u : return
#fn - i : return  A A I E
