fn - 0x35 : ~/bin/keyboard_backlight # skhd -k "f18" # only-switch : key light toggle, esc
# fn - f* : special features / standard function keys (stock)

fn - 0x0A : return # ^
fn - 1 : return
fn - 2 : skhd -t "²"
fn - 3 : skhd -t "³"
fn - 4 : return
fn - 5 : return
fn - 6 : return
fn - 7 : skhd -k "alt - 8" # {
fn - 8 : skhd -k "alt - 5" # [
fn - 9 : skhd -k "alt - 6" # ]
fn - 0 : skhd -k "alt - 9" # }
fn - 0x1B : skhd -k "shift + alt - 7" # \
fn - 0x18 : return # ´
# fn - backspace/delete : forward delete (stock)

fn - tab : return
fn - q : skhd -k "alt - l" # @, quick note (overwritten here & deactivated in systemSettings/Keyboard/KeyboardShortcuts/.. like many others not mentioned specifically)
fn - w : ~/bin/custom_wake
fn - e : skhd -k "alt - e" # €, show the character viewer, from which you can choose emoji and other symbols (stock)
fn - r : ~/bin/radio # skhd -k "hyper - r" # only-switch : radio
fn - t : return # Alternative zu OnlySwitch Pomodoro Timer # skhd -k "hyper - t" # only-switch : timer (pomodoro)
fn - z : return
fn - u : return
fn - i : return
fn - o : return
fn - p : return
fn - 0x21 : return # ü
fn - 0x1E : skhd -k "alt - n" # +, ~

fn - a : return # show or hide the dock (stock)
fn - s : pmset sleepnow
fn - d : return # start or stop dictation (stock)
fn - f : skhd -k "ctrl + cmd - f" # 'native fullscreen' (stock), ctrl + cmd - f works more universal and reliable than fn - f (also stock)
fn - g : return
fn - h : return
fn - j : return
fn - k : return
fn - l : return
fn - 0x29 : return # ö
fn - 0x27 : return # ä
# fn - 0x2A : cf. 'q1_specific', #
# fn - return : /opt/homebrew/bin/yabai -m query --windows --window > ~/Desktop/yabai_debug.txt #.../enter = 0x4C: keysmith : yabai_debugging [pent_to_fn_enter]
# fn - return : yabai -m query --windows --window | jq '."frame"."y"' > ~/Desktop/yabai_debug.txt
fn - return : SIDPOS=$(yabai -m query --windows --window); echo "$SIDPOS" | jq '."frame"."x"' > ~/Desktop/yabai_debug.txt
    0x4C : skhd -k "fn - return"

fn - 0x32 : skhd -k "alt - 7" # <, |
fn - y : /opt/homebrew/bin/yabai --restart-service
fn - x : /opt/homebrew/bin/sketchybar --reload # --update
# fn - c : show/hide control center (stock)
fn - v : return
fn - b : ~/bin/macos_background_noise # skhd -k "hyper - b" # only-switch : background noise
# fn - n : show(/hide) notification center (stock)
fn - m : skhd -k "alt - m" # µ
# fn - 0x2B : sysdiagnose tailspin (stock), ,
# fn - 0x2F : sysdiagnose (stock), .
fn - 0x2C : return # -

fn - space : skhd -k "space"
# fn - arrows : home/pageup/pagedown/end (stock)
