{
  "$schema" : "https:\/\/schema.linearmouse.app\/0.10.2",
  "schemes" : [
    {
      "buttons" : {
        "clickDebouncing" : {
          "buttons" : [
            3,
            4,
            2
          ],
          "timeout" : 500
        },
        "mappings" : [
          {
            "action" : "mouse.button.left",
            "button" : 0,
            "control" : true
          },
          {
            "action" : {
              "run" : "\/opt\/homebrew\/bin\/skhd -k 'fn - y'"
            },
            "button" : 3,
            "control" : true
          },
          {
            "action" : {
              "run" : "\/bin\/bash -c '\/Users\/q\/.config\/sketchybar\/plugins\/click_week.sh'"
            },
            "button" : 4,
            "control" : true
          },
          {
            "action" : {
              "keyPress" : [
                "command",
                "0"
              ]
            },
            "button" : 3,
            "option" : true
          },
          {
            "action" : "smartZoom",
            "button" : 4,
            "option" : true
          },
          {
            "action" : {
              "keyPress" : [
                "option",
                "command",
                "arrowLeft"
              ]
            },
            "button" : 0,
            "command" : true,
            "option" : true
          },
          {
            "action" : {
              "keyPress" : [
                "option",
                "command",
                "arrowRight"
              ]
            },
            "button" : 1,
            "command" : true,
            "option" : true
          }
        ],
        "universalBackForward" : true
      },
      "if" : {
        "device" : {
          "category" : "mouse",
          "productID" : "0x82a",
          "productName" : "Microsoft Pro Intellimouse",
          "vendorID" : "0x45e"
        }
      },
      "pointer" : {
        "acceleration" : 0,
        "speed" : 0.5
      },
      "scrolling" : {
        "acceleration" : 2.1,
        "distance" : "auto",
        "modifiers" : {
          "option" : {
            "scale" : 10,
            "type" : "changeSpeed"
          }
        },
        "reverse" : {
          "vertical" : true
        },
        "speed" : 21
      }
    },
    {
      "buttons" : {
        "mappings" : [
          {
            "action" : "mouse.button.left",
            "button" : 0,
            "command" : false,
            "control" : true,
            "option" : false,
            "shift" : false
          }
        ]
      },
      "if" : {
        "device" : {
          "category" : "trackpad",
          "productID" : "0x0",
          "productName" : "Apple Internal Keyboard \/ Trackpad",
          "vendorID" : "0x0"
        }
      },
      "pointer" : {
        "acceleration" : 2,
        "speed" : 1
      },
      "scrolling" : {
        "modifiers" : {
          "option" : {
            "scale" : 7,
            "type" : "changeSpeed"
          }
        }
      }
    },
    {
      "buttons" : {
        "mappings" : [

        ]
      },
      "if" : {
        "device" : {
          "category" : "mouse",
          "productID" : "0x27da",
          "productName" : "Karabiner DriverKit VirtualHIDPointing 1.8.0",
          "serialNumber" : "pqrs.org:Karabiner-DriverKit-VirtualHIDPointing",
          "vendorID" : "0x16c0"
        }
      }
    }
  ]
}